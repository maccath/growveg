{
  "name": "maccath/growveg",
  "description": "Wrapper for GrowVeg REST API",
  "type": "library",
  "keywords": [
    "REST",
    "API",
    "wrapper",
    "GrowVeg"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Katy Ereira",
      "email": "katy.ereira@gmail.com",
      "homepage": "https://github.com/maccath"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "alexanderpas/http-enum": "^1.0",
    "nyholm/psr7": "^1.8",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "ramsey/collection": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Maccath\\GrowVeg\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maccath\\GrowVeg\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "squizlabs/php_codesniffer": "^3.7",
    "slevomat/coding-standard": "^8.14"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
